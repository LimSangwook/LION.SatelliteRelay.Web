                    <th style="width: 5%">FILTER TYPE </th>
                    <th style="width: 5%">SIMPLE FILTER</th>
                    <th style="width: 5%">REGEXP FILTER</th>
                  </tr>
                </thead>
                <tbody>
                  <tr >
                    <td><select style="width:100%;height:30px" name="FILTER_TYPE" id="Create_FILTER_TYPE"><% include ./optionsFilterTYPE.ejs%></select></td>
                    <td><input style="width:100%;height:32px" name="FILTER_SIMPLE" id="Create_FILTER_SIMPLE"></td>
                    <td><input style="width:100%;height:32px" name="FILTER_REGEXP" id="Create_FILTER_REGEXP"></td>
                  </tr>
                </tbody>
                <thead >
                  <tr >
                    <th style="width: 5%">SCHEDULE TYPE </th>
                    <th style="width: 5%">SCHEDULE TIME </th>
                  </tr>
                </thead>
                <tbody>
                  <tr >
                    <td><select style="width:100%;height:30px" name="SCHEDULE_TYPE" id="Create_SCHEDULE_TYPE"><% include ./optionsScheduleTYPE.ejs%></select></td>
                    <td><input style="width:100%;height:32px" name="SCHEDULE_TIME" id="Create_SCHEDULE_TIME" placeholder="24:00:00"></td>
                  </tr>
                </tbody>
              </table>
            </font>
<font color="blue"><b>APPENDIX :</b></font>
            <font size="1">
              <table class="table table-sm">
                <thead>
                  <tr>
                    <th style="width: 5%">COORD UL </th>
                    <th style="width: 5%">COORD LR </th>
                    <th style="width: 5%">REG DATE </th>
                    <th style="width: 5%">PIXEL ROW </th>
                    <th style="width: 5%">PIXEL_COL </th>
                    <th style="width: 5%">DATA TYPE </th>
                    <th style="width: 5%">DATA FORMAT </th>
                    <th style="width: 5%">PROJECTION </th>
                  </tr>
                </thead>
                <tbody>
                  <tr>
                    <td><input style="width:100%;height:32px" name="COORD_UL" id="Create_COORD_UL"></td>
                    <td><input style="width:100%;height:32px" name="COORD_LR" id="Create_COORD_LR"></td>
                    <td><input style="width:100%;height:32px" name="REG_DATE" id="Create_REG_DATE"></td>
                    <td><input style="width:100%;height:32px" name="PIXEL_ROW" id="Create_PIXEL_ROW"></td>
                    <td><input style="width:100%;height:32px" name="PIXEL_COL" id="Create_PIXEL_COL"></td>
                    <td><select style="width:100%;height:30px" name="DATA_TYPE" id="Create_DATA_TYPE"><% include ./optionsDataTYPE.ejs%></select></td>
                    <td><select style="width:100%;height:30px" name="DATA_FORMAT" id="Create_DATA_FORMAT"><% include ./optionsDataFormat.ejs%></select></td>
                    <td><input style="width:100%;height:32px" name="PROJECTION" id="Create_PROJECTION"></td>
                  </tr>
                </tbody>
                <thead>
                  <tr>
                    <th style="width: 5%">QUICK LOOK </th>
                    <th style="width: 5%">DATA GBN </th>
                    <th style="width: 5%">DATA AN GBN </th>
                    <th style="width: 5%">FILE STATUS </th>
                    <th style="width: 5%">MOUNT POINT </th>
                    <th style="width: 5%">DATA OPEN </th>
                  </tr>
                </thead>
                <tbody>
                  <tr>
                    <td><input style="width:100%;height:32px" name="QUICK_LOOK" id="Create_QUICK_LOOK"></td>
                    <td><select style="width:100%;height:30px" name="DATA_GBN" id="Create_DATA_GBN"><% include ./optionsTBDataGBN.ejs%></select></td>
                    <td><select style="width:100%;height:30px" name="DATA_AN_GBN" id="Create_DATA_AN_GBN"><% include ./optionsTBDataANGBN.ejs%></select></td>
                    <td><input style="width:100%;height:32px" name="FILE_STATUS" id="Create_FILE_STATUS"></td>
                    <td><input style="width:100%;height:32px" name="MOUNT_POINT" id="Create_MOUNT_POINT"></td>
                    <td><input style="width:100%;height:32px" name="DATA_OPEN" id="Create_DATA_OPEN"></td>
                  </tr>
                </tbody>
                <thead>
                </table>
              </font>
            </div>
          </div>
          <div class="modal-footer">
            <button type="button" data-dismiss="modal">Cancle</button>
            <input type="button" value="Create" onclick="createFormSubmit()" />
          </div>
        </form>
      </div>
    </div>
  </div>


