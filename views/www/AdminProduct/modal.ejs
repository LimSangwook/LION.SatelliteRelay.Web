                  <tr >
                    <td><input style="width:100%;height:32px" name="SOURCEPATH" id="Create_SOURCEPATH"></td>
                    <td><input style="width:100%;height:32px" name="TARGETPATH" id="Create_TARGETPATH"></td>
                    <td><select style="width:100%;height:30px" name="TARGETPATH_TYPE" id="Create_TARGETPATH_TYPE"><% include ./optionsTargetPathType.ejs%></select></td>
                  </tr>
                </tbody>
                <thead >
